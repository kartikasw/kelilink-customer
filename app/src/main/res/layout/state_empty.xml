<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_vertical">

    <TextView
        android:id="@+id/se_tv_title"
        android:textStyle="bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/title_store_empty"
        android:textAppearance="@style/TextAppearance.Kelilink.Body1" />

    <TextView
        android:id="@+id/se_tv_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/content_store_empty"
        android:gravity="center"
        android:layout_marginTop="@dimen/margin_component"
        android:layout_marginHorizontal="@dimen/margin_layout_more"
        android:textAppearance="@style/TextAppearance.Kelilink.Body2"/>

</LinearLayout>